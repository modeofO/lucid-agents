{
  "name": "@lucid-agents/agent-kit",
  "version": "1.3.0",
  "description": "Build typed agent HTTP apps",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./utils/axllm": {
      "types": "./dist/utils/axllm.d.ts",
      "import": "./dist/utils/axllm.js",
      "default": "./dist/utils/axllm.js"
    },
    "./utils/x402": {
      "types": "./dist/utils/x402.d.ts",
      "import": "./dist/utils/x402.js",
      "default": "./dist/utils/x402.js"
    },
    "./erc8004": {
      "types": "./dist/erc8004.d.ts",
      "import": "./dist/erc8004.js",
      "default": "./dist/erc8004.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "AGENTS.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepare": "bun run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --build tsconfig.json && tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@lucid-agents/agent-core": "workspace:*",
    "zod": "catalog:",
    "@lucid-dreams/agent-auth": "latest",
    "@lucid-dreams/client": "latest",
    "@lucid-agents/agent-kit-identity": "workspace:*",
    "x402-fetch": "catalog:",
    "x402": "^0.7.1",
    "viem": "^2.38.5",
    "@ax-llm/ax": "^14.0.37",
    "@ax-llm/ax-tools": "^14.0.37"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@lucid-agents/eslint-config": "workspace:*",
    "@lucid-agents/prettier-config": "workspace:*"
  }
}
