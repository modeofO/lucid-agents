{
  "id": "axllm",
  "name": "AxLLM Bun Agent",
  "description": "Fully-featured Bun HTTP agent with @lucid-dreams/agent-kit and @ax-llm/ax wired up.",
  "adapters": ["hono", "tanstack"],
  "package": {
    "dependencies": {}
  },
  "wizard": {
    "prompts": [
      {
        "key": "AGENT_DESCRIPTION",
        "type": "input",
        "message": "How would you describe your agent?",
        "defaultValue": "Agent with LLM integration"
      },
      {
        "key": "AGENT_VERSION",
        "type": "input",
        "message": "What version should the agent start at?",
        "defaultValue": "0.1.0"
      },
      {
        "key": "PAYMENTS_FACILITATOR_URL",
        "type": "input",
        "message": "Facilitator URL",
        "defaultValue": "https://facilitator.daydreams.systems"
      },
      {
        "key": "PAYMENTS_NETWORK",
        "type": "input",
        "message": "Payment network identifier",
        "defaultValue": "base-sepolia"
      },
      {
        "key": "PAYMENTS_RECEIVABLE_ADDRESS",
        "type": "input",
        "message": "Receivable address (address that receives payments)",
        "defaultValue": ""
      },
      {
        "key": "PAYMENTS_DEFAULT_PRICE",
        "type": "input",
        "message": "Default price in base units",
        "defaultValue": "1000"
      },
      {
        "key": "PRIVATE_KEY",
        "type": "input",
        "message": "Wallet private key (leave empty to add later)",
        "defaultValue": ""
      }
    ]
  }
}
