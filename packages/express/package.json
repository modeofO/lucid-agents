{
  "name": "@lucid-agents/express",
  "version": "0.1.0",
  "description": "Express adapter for Lucid agent runtime",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@lucid-agents/core": "workspace:*",
    "@lucid-agents/payments": "workspace:*",
    "@lucid-agents/types": "workspace:*",
    "express": "^4.19.2",
    "x402": "^0.7.1",
    "x402-express": "^0.7.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
